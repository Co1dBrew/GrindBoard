{
    "env": {
        "node": true,
        "es2022": true,
        "browser": true
    },
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "semi": ["error", "always"],
        "quotes": ["error", "double"],
        "indent": ["error", 4]
    }
}
